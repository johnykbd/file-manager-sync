{
    "name": "KBD Stream",
    "short_name": "KBD Stream",
    "description": "Personal Media Streaming Server",
    "theme_color": "#09090b",
    "background_color": "#09090b",
    "display": "standalone",
    "display_override": [
        "fullscreen",
        "standalone"
    ],
    "orientation": "any",
    "share_target": {
        "action": "/api/upload?share=true",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "files": [
                {
                    "name": "file",
                    "accept": [
                        "image/*",
                        "video/*"
                    ]
                }
            ]
        }
    },
    "scope": "/",
    "start_url": "/",
    "icons": [
        {
            "src": "/icon-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "/icon-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "/icon-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ]
}